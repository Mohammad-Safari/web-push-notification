{
  "/api/*": {
    "target": "https://spring-mvc:8443/",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug"
  },
  "/event/*": {
    "target": "https://spring-mvc:8443/",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug"
  },
  "/app/*": {
    "target": "https://spring-mvc:4200/",
    "secure": false,
    "pathRewrite": {
      "^/app": ""
    },
    "logLevel": "debug"
  }
}
